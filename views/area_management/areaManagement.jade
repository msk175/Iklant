                                                a(href="JavaScript:approveOrRejectArea('#{areaCodeId[i]}','#{officeIds[i]}',1);",rel="external",data-theme="b",data-icon='check',id='approve_#{areaName[i]}',data-role='button',data-mini="true", title='Approve', data-inline="true") Approve
                                    -}
                    -}
                    -else{
                        p(data-theme="e",align='center',class='error-message global-font-details') No records found
                    -}
                -}
                -else{
                    p(data-theme="e",align='center',class='error-message global-font-details') No records found
                -}
                input(type='hidden',id='operationId',name='operationId',value=selectedOperationId)
