extends ../layout
include ../header

block title
    title= 'Iklant-Manage Area'

block scripts
    include ../headerJqueryMobile-1
    link(rel='stylesheet', href='/stylesheets/iklant-table-layout.css')
    script(type='text/javascript', src='/javascripts/area_management/areaManagement.js')

block content
    form(id='areaManagementForm')
        div(data-role='page', data-theme='b')
            +header("Area Management",["menu","logout"],'',[])
            div(data-role="navbar", data-grid="d")
                ul
                    -for(var i=0;i< operationId.length;i++) {
                        -if(operationId[i] == selectedOperationId) {
                            li
                                a(href="JavaScript:operationSubmitForAreaMgmt('#{operationId[i]}');", rel="external", data-role="tab", id = "#{operationName[i]}", class="ui-btn-active") #{operationName[i]}
                        -}
                        -else if(roleId == constantsObj.getSMHroleId() && operationId[i] != constantsObj.getAreaCreationOperationId()){
                            -if(operationId[i] == constantsObj.getAssignAreaOperationId()){
                                li
                                    a(href="JavaScript:operationSubmitForAreaMgmt('#{operationId[i]}');", rel="external", id = "#{operationName[i]}", data-role="tab") Release Area
                            -}
                            -else{
                                li
                                    a(href="JavaScript:operationSubmitForAreaMgmt('#{operationId[i]}');", rel="external", id = "#{operationName[i]}", data-role="tab") #{operationName[i]}
                            -}
                        -}
                        -else if(roleId == constantsObj.getBMroleId()){
                            li
                                a(href="JavaScript:operationSubmitForAreaMgmt('#{operationId[i]}');", rel="external", id = "#{operationName[i]}", data-role="tab") #{operationName[i]}
                        -}
                    -}
            div(data-role="bar")
                div(data-role="fieldcontain")
            -if(roleId == constantsObj.getSMHroleId()){
                div(data-role="content", data-theme="b", data-overlay-theme="a",class="ui-grid-a",style='width:100%;')
                    div(class='ui-block-e',style='width:50%;',align="right")
                        div(data-role='fieldcontain')
                            label(for="listoffice",style='font-weight:bold') Select office
                    div(class='ui-block-e',style='width:50%;',align="left")
                        div(data-role='fieldcontain')
                            select(name='listoffice', id='listoffice', data-theme='b', data-icon='grid', data-inline='true', data-mini='true',onchange='retrieveAreaList()')
                                option(value='-1') All
                                -for(i=0;i<officeIdArray.length;i++){
                                    -if(officeId == officeIdArray[i] && officeId != constantsObj.getApexHeadOffice()){
                                        option(value=officeIdArray[i],selected="selected") #{officeNameArray[i]}
                                    -}
                                    -else if(officeIdArray[i] != constantsObj.getApexHeadOffice()){
                                        option(value=officeIdArray[i]) #{officeNameArray[i]}
                                    -}
                                - }
            -}

            div
                p(id='successMessage',align="center",class='success-message global-font-details') #{statusMessage}
                -if(areaCodeId.length>0){
                    -if(roleId == constantsObj.getBMroleId()){
                        table(id='areaListDiv')
                            thead
                                tr
                                    th S. No
                                    th Area Code
                                    th Area Name
                                    th Status
                                    th Actions
                                    - count = 1;
                                    -for(var i=0;i< areaCodeId.length;i++){
                                        -if((statusArray[i] == 0 && isVerified[i] == 0) || (statusArray[i] == 1)){
                                            tr
                                                td(class='center-text') #{count}
                                                -if(areaCode[i] != null)
                                                    td(class='center-text') #{areaCode[i]}
                                                -else
                                                    td(class='center-text') -
                                                td #{areaName[i]}
                                                -if(foNames[i]){
                                                    td(style='padding-left:2em')
                                                        -if(releaseRequests[i] != 1)
                                                            label Assigned - #{foNames[i]}
                                                        -else
                                                            label Requested to Release
                                                    td(style='color:red;padding-left:2em')
                                                        -if(releaseRequests[i] != 1)
                                                            a(href="JavaScript:populateAreaDetails('#{areaCodeId[i]}','#{officeIds[i]}','BM',#{statusArray[i]});",rel="external",data-theme="b",data-icon='gear', data-role='button', id="edit_#{areaName[i]}",data-mini="true", title='Edit', data-inline="true") Edit
                                                            a(href="JavaScript:releaseArea(#{areaCodeId[i]},#{foIds[i]},1)",rel="external",data-theme="a",data-icon='check', data-role='button',data-mini="true", id="release_#{areaName[i]}" title='Re-assign', data-inline="true") Release
                                                        -else
                                                            a(href="JavaScript:populateAreaDetails('#{areaCodeId[i]}','#{officeIds[i]}','BM',#{statusArray[i]});",rel="external",data-theme="b",data-icon='gear', data-role='button',data-mini="true",id="edit_#{areaName[i]}",title='Edit', data-inline="true",style='width:53%') Edit
                                                -}
                                                -else if(statusArray[i] == 0 && isVerified[i] == 0){
                                                    td(class='error-message global-font-details',style='padding-left:2em') Rejected by<br/> Regional Manager
                                                    td(style='padding-left:2em')
                                                        a(href="JavaScript:populateAreaDetails('#{areaCodeId[i]}','#{officeIds[i]}','BM',#{statusArray[i]});",rel="external",data-theme="b",data-icon='gear', data-role='button',data-mini="true",id="edit_#{areaName[i]}",title='Edit', data-inline="true",style='width:53%') Edit
                                                -}
                                                -else if(statusArray[i] == 1 && isVerified[i] == 0){
                                                    td(style='color:#000000;padding-left:2.5em;font-weight:bolder;font-size:14px') Waiting for <br/> Regional Manager approval
                                                    td(style='padding-left:2em')
                                                        a(href="JavaScript:populateAreaDetails('#{areaCodeId[i]}','#{officeIds[i]}','BM',#{statusArray[i]});",rel="external",data-theme="b",data-icon='gear', data-role='button',data-mini="true",id="edit_#{areaName[i]}", title='Edit', data-inline="true",style='width:53%') Edit
                                                -}
                                                -else{
                                                    td(class='success-message global-font-details',style='padding-left:2em') Unassigned
                                                    td(style='padding-left:2em')
                                                        a(href="JavaScript:populateAreaDetails('#{areaCodeId[i]}','#{officeIds[i]}','BM',#{statusArray[i]});",rel="external",data-theme="b",data-icon='gear', data-role='button',data-mini="true",id="edit_#{areaName[i]}", title='Edit', data-inline="true",style='width:53%') Edit
                                                -}
                                        -count++;
                                        -}
                                    -}
                    -}
                    -else if(isVerified.indexOf(0) > -1 && roleId == constantsObj.getSMHroleId()){
                        table(id='areaListDiv')
                            thead
                                tr
                                    th S. No
                                    th Area Name
                                    th Actions
                                    -for(var i=0;i< areaCodeId.length;i++){
                                        tr
                                            td(style ="text-align:center") #{i+1}
                                            td #{areaName[i]}
                                            td(style ="text-align:center",colspan='2')
