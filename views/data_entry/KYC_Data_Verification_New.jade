extends ../layout
include ../header

block title
    title= 'IKLANT-KYCDataVerification'

block scripts
    link(rel='stylesheet', href='/stylesheets/jquery-ui.css')
    include ../headerJqueryMobile-1
    link(rel='stylesheet', href='/stylesheets/jqx.base.css')
    link(rel='stylesheet', href='/stylesheets/jquery.mobile.structure-1.1.1.min.css')
    link(rel='stylesheet', href='/stylesheets/popup.css')
    link(rel="stylesheet", href="/stylesheets/jquery.iviewer.css")
    link(rel="stylesheet", href="/stylesheets/iklant-table-layout.css")
    script(type='text/javascript', src='/javascripts/jquery-ui.js')
    script(type="text/javascript", src="/javascripts/jquery.mousewheel.min.js")
    script(type="text/javascript", src="/javascripts/jquery.iviewer.js")
    script(type='text/javascript', src='/javascripts/jqxcore.js')
    script(type='text/javascript', src="/javascripts/jqxsplitter.js")
    script(type='text/javascript', src="/javascripts/data_entry/kycValidation.js")
    script(type='text/javascript', src='/javascripts/commonCustom.js')

block content
    form(id='BMFormId')
        div(data-role='page', data-theme='b', data-position="fixed")
            +header("KYC Data Verification",["backJavascript","logout"],"cancelDataVerification",[])
            div(data-theme="b" )
                p(id='errorLabelId',align="center",class='error-message global-font-details')
                -if (successMessage.indexOf('failed') > -1){
                    p(align="center",class='error-message global-font-details') #{successMessage}
                -}
                -else{
                    p(id='successMessage',align="center",class='success-message global-font-details') #{successMessage}
                -}
                div(data-role="horizontal", data-theme="b", data-overlay-theme="b", data-inline="true",class="ui-bar ui-grid-a",style='width:80%')
                    div(class='ui-block-a',style='width:10%')
                        div(data-role="fieldcontain")
                        label(for="groupName") Group Name
                    div(class='ui-block-b',style='width:10%')
                        div(data-role="fieldcontain")
                        h3 #{prospectGroup.getGroup_name()}
                        input(type="hidden",id="groupNameHidden",name="GroupNameHidden",value="#{prospectGroup.getGroup_name()}")
                        input(type="hidden" ,id="groupIdHidden", name="groupIdHiddenName",value="#{groupId}")
                    div(class='ui-block-c',style='width:10%')
                        div(data-role="fieldcontain")
                        label(for="centerName", data-inline="true") Center Name
                    div(class='ui-block-d',style='width:40%')
                        div(data-role="fieldcontain")
                            a(href='JavaScript:clientCurrentStatus();',id="clientCurrentStatus" , data-rel='popup', data-position-to='window', data-transition='pop', data-inline='true', data-mini='true')  #{prospectGroup.getCenter_name()}
                        input(type="hidden", name="Center Name", id="centerName", readonly='readonly', value="#{prospectGroup.getCenter_name()}")
                    div(class='ui-block-e',style='width:10%')
                        div(data-role="fieldcontain")
                        label Member Name
                    div(class='ui-block-e',style='width:20%')
                        div(data-role="fieldcontain")
                            select(name='MemberName',rel = "external",onchange="JavaScript:memberNameOnchange(0)", data-mini="true" ,id="memberName",data-inline="true",autofocus="autofocus", data-theme='b', data-icon='arrow-d', data-native-menu="true")
                                option(value="0")    Select
                                -for(i=0;i< clientNames.length;i++){
                                    -if(clientId[i]==clientNameID)
                                        option(value=clientId[i],selected='selected') #{clientNames[i]}
                                    -else
                                        option(value=clientId[i]) #{clientNames[i]}
                                - }
                            input(type="hidden",id="memberNameIdHidden",name="MemberNameIdHidden",value="")
                            input(type="hidden",id="flagHiddenId",name="flagHidden",value= flag )
                            //input(type='checkbox', name='rejectMemberDV', id='rejectMemberDV', data-inline="true",data-mini="true",onclick="rejectDVValidation()",align="left")
                            //label(for='rejectMemberDV') Reject Member
                    div(class='ui-block-a updatedDetails',style='width:13%')
                        div(data-role="fieldcontain")
                            label Hold History
                    div(class='ui-block-b updatedDetails',style='width:20%')
                        div(data-role="fieldcontain")
                            //-a(href='JavaScript:checkHoldHistoryMember();' , id='holdHistoryId', rel="external",data-theme="b",data-role='button', title=' Hold ', data-inline="true", data-mini="true")
                            a(href="JavaScript:JavaScript:checkHoldHistoryMember();",id='custommainTab',data-theme="b",data-rel="popup",data-transition="slide",data-position-to="window",data-mini="true")
                                img(id='groupIcon', style="margin:5px 10px 0 0; text-align:right;",width="40px",height="40px",src='/images/member.png')
                    div(class='ui-block-c updatedDetails',style='width:13%')
                        div(data-role="fieldcontain")
                        label(for="kycUpdatedBy") KYC Done By
                    div(class='ui-block-d updatedDetails',style='width:20%')
                        div(data-role="fieldcontain")
                        h3(id="kycUpdatedBy") #{prospectClient.getKYC_Updated_By()}
                    div(class='ui-block-e updatedDetails',style='width:15%')
                        div(data-role="fieldcontain")
                            input(type='checkbox', name='rejectMemberDV', id='rejectMemberDV', data-inline="true",data-mini="true",onclick="rejectDVValidation()",align="left")
                            label(for='rejectMemberDV') Reject Member
                    div(class='ui-block-c updatedDetails',style='width:2%')
                    div(class='ui-block-d updatedDetails',style='width:80%')
                        div(data-role="fieldcontain",id="rejectReasonDiv")
                            label(for='reasonForRejection') Reason for Reject
                            input(type='text', id='reasonForRejection', name='reasonForRejection',autocomplete="off",onkeyup='forAlphaNumericWithSpace(this)')
                    //a(href='JavaScript:downloadRequestedDocument()', rel="external", data-role='button', title=' Download requested Document', data-inline="true", data-mini="true",style='width:18%') Download Documents
                    -if(prospectClientPersonalObject.getCommentsByRM()) {
                        div(class='ui-block-a updatedDetails',style='width:19%')
                            div(data-role="fieldcontain")
                            label(for="dataVerifiedBy") Comments By RM
                        div(class='ui-block-b updatedDetails',style='width:35%')
                            div(data-role="fieldcontain")
                            h3(id="kycUpdatedBy") #{prospectClientPersonalObject.getCommentsByRM()}
                    - }
                    -if(prospectClientPersonalObject.getRemarksByDV()) {
                        div(class='ui-block-a updatedDetails',style='width:10%')
                            div(data-role="fieldcontain")
                            label(for="dvQueryLabel") Remarks By DV
                        div(class='ui-block-b updatedDetails',style='width:45%')
                            div(data-role="fieldcontain")
                            h3(id="dvQueryLabelValue") #{prospectClientPersonalObject.getRemarksByDV()}
                    - }
                    -if(prospectClientPersonalObject.getRemarks()) {
                        div(class='ui-block-a updatedDetails',style='width:10%')
                            div(data-role="fieldcontain")
                            label(for="bmQueryLabel") Remarks By BM
                        div(class='ui-block-b updatedDetails',style='width:45%')
                            div(data-role="fieldcontain")
                            h3(id="bmQueryLabelValue") #{prospectClientPersonalObject.getRemarks()}
                    - }

            hr
